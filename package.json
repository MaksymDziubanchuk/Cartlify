{
  "name": "cartlify",
  "version": "1.0.0",
  "description": "Cartlify - E-commerce pet-project with Fastify, GraphQL, Prisma, Supabase, Cloudinary, and SSR Handlebars.",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "test": "jest --runInBand --detectOpenHandles",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "keywords": [
    "fastify",
    "apollo",
    "graphql",
    "prisma",
    "postgresql",
    "ecommerce",
    "cartlify"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@prisma/client": "^6.0.0",
    "fastify": "^5.0.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/static": "^6.12.0",
    "socket.io": "^4.8.1",
    "graphql": "^16.9.0",
    "handlebars": "^4.7.8",
    "@supabase/supabase-js": "^2.43.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "cloudinary": "^2.5.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.16.5",
    "@types/supertest": "^2.0.16",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^6.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
