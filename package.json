{
  "name": "cartlify",
  "type": "module",
  "version": "1.0.0",
  "description": "Cartlify - E-commerce pet-project with Fastify, GraphQL, Prisma, Supabase, Cloudinary, and SSR Handlebars.",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx watch -r tsconfig-paths/register src/index.ts",
    "start": "tsx -r tsconfig-paths/register dist/index.js",
    "build": "tsc -p tsconfig.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "test": "jest --runInBand --detectOpenHandles",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "fastify",
    "apollo",
    "graphql",
    "prisma",
    "postgresql",
    "ecommerce",
    "cartlify"
  ],
  "author": "Maksym Dziubanchuk",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/session": "^11.1.0",
    "@fastify/static": "^8.2.0",
    "@prisma/client": "^6.0.0",
    "@supabase/supabase-js": "^2.43.0",
    "@types/pino": "^7.0.5",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.0",
    "dotenv": "^16.4.5",
    "fastify": "^5.6.0",
    "graphql": "^16.9.0",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.11.0",
    "pino-pretty": "^13.1.1",
    "socket.io": "^4.8.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.14",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^6.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
